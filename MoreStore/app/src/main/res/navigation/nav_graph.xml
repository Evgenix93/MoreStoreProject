            android:id="@+id/action_cabinetFragment_to_firstLaunchFragment3"
            app:destination="@id/firstLaunchFragment" />
        <action
            android:id="@+id/action_cabinetFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
        <action
            android:id="@+id/action_cabinetFragment_to_cabinetGuestFragment"
            app:destination="@id/cabinetGuestFragment" />
    </fragment>
    <dialog
        android:id="@+id/resumeRegisterDialog"
        android:name="com.project.morestore.ResumeRegisterDialog"
        android:label="ResumeRegisterDialog" >
        <argument
            android:name="isEmail"
            app:argType="boolean" />
        <action
            android:id="@+id/action_resumeRegisterDialog_to_registration3Fragment"
            app:destination="@id/registration3Fragment" />
        <argument
            android:name="phoneOrEmail"
            app:argType="string" />
        <argument
            android:name="code"
            app:argType="integer" />
        <argument
            android:name="userId"
            app:argType="integer" />
        <action
            android:id="@+id/action_resumeRegisterDialog_to_mainFragment"
            app:destination="@id/mainFragment" />
    </dialog>
    <fragment
        android:id="@+id/loginSocialFragment"
        android:name="com.project.morestore.fragments.LoginSocialFragment"
        android:label="LoginSocialFragment" >
        <argument
            android:name="socialType"
            app:argType="string" />
        <action
            android:id="@+id/action_loginSocialFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
        <action
            android:id="@+id/action_loginSocialFragment_to_registration3Fragment"
            app:destination="@id/registration3Fragment" />
        <action
            android:id="@+id/action_loginSocialFragment_to_onboarding1Fragment"
            app:destination="@id/onboarding1Fragment" />
    </fragment>
    <fragment
        android:id="@+id/filterSizesFragment"
        android:name="com.project.morestore.fragments.FilterSizesFragment"
        android:label="FilterSizesFragment" />
    <fragment
        android:id="@+id/filterMaterialsFragment"
        android:name="com.project.morestore.fragments.FilterMaterialsFragment"
        android:label="FilterMaterialsFragment" />
    <fragment
        android:id="@+id/productDetailsFragment"
        android:name="com.project.morestore.fragments.ProductDetailsFragment"
        android:label="ProductDetailsFragment" >
        <argument
            android:name="product"
            app:argType="com.project.morestore.models.Product"
            app:nullable="true" />
        <argument
            android:name="productId"
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/changeRegionFragment"
        android:name="com.project.morestore.fragments.ChangeRegionFragment"
        android:label="ChangeRegionFragment" >
        <action
            android:id="@+id/action_changeRegionFragment_to_autoLocationFragment"
            app:destination="@id/autoLocationFragment" />
    </fragment>
    <fragment
        android:id="@+id/filterForWhoFragment"
        android:name="com.project.morestore.fragments.FilterForWhoFragment"
        android:label="FilterForWhoFragment" />
    <fragment
        android:id="@+id/filterConditionFragment"
        android:name="com.project.morestore.fragments.FilterConditionFragment"
        android:label="FilterConditionFragment" />
    <fragment
        android:id="@+id/filterStyleFragment"
        android:name="com.project.morestore.fragments.FilterStyleFragment"
        android:label="FilterStyleFragment" />
    <fragment
        android:id="@+id/filterProductStatusFragment"
        android:name="com.project.morestore.fragments.FilterProductStatusFragment"
        android:label="FilterProductStatusFragment" />
    <fragment
        android:id="@+id/brandsFragment"
        android:name="com.project.morestore.fragments.FilterBrandsFragment"
        android:label="BrandsFragment" />
    <fragment
        android:id="@+id/regionsFragment"
        android:name="com.project.morestore.fragments.FilterRegionsFragment"
        android:label="RegionsFragment" />
    <fragment
        android:id="@+id/categoriesFragment"
        android:name="com.project.morestore.fragments.FilterCategoriesFragment"
        android:label="CategoriesFragment" />
    <fragment
        android:id="@+id/filterFragment"
        android:name="com.project.morestore.fragments.FilterFragment"
        android:label="FilterFragment" >
        <action
            android:id="@+id/action_filterFragment_to_categoriesFragment"
            app:destination="@id/categoriesFragment" />
        <action
            android:id="@+id/action_filterFragment_to_brandsFragment"
            app:destination="@id/brandsFragment" />
        <action
            android:id="@+id/action_filterFragment_to_regionsFragment"
            app:destination="@id/regionsFragment" />
        <action
            android:id="@+id/action_filterFragment_to_filterSizesFragment"
            app:destination="@id/filterSizesFragment" />
        <action
            android:id="@+id/action_filterFragment_to_filterMaterialsFragment"
            app:destination="@id/filterMaterialsFragment" />
        <action
            android:id="@+id/action_filterFragment_to_filterProductStatusFragment"
            app:destination="@id/filterProductStatusFragment" />
        <action
            android:id="@+id/action_filterFragment_to_filterForWhoFragment"
            app:destination="@id/filterForWhoFragment" />
        <action
            android:id="@+id/action_filterFragment_to_filterStyleFragment"
            app:destination="@id/filterStyleFragment" />
        <action
            android:id="@+id/action_filterFragment_to_filterConditionFragment"
            app:destination="@id/filterConditionFragment" />
        <action
            android:id="@+id/action_filterFragment_to_colorsFragment"
            app:destination="@id/colorsFragment" />
        <action
            android:id="@+id/action_filterFragment_to_filterKidsSizesFragment"
            app:destination="@id/filterKidsSizesFragment" />
        <action
            android:id="@+id/action_filterFragment_to_filterShoosSizesFragment"
            app:destination="@id/filterShoosSizesFragment" />
    </fragment>
    <fragment
        android:id="@+id/colorsFragment"
        android:name="com.project.morestore.fragments.FilterColorsFragment"
        android:label="ColorsFragment" />
    <fragment
        android:id="@+id/autoLocationFragment"
        android:name="com.project.morestore.fragments.AutoLocationFragment"
        android:label="AutoLocationFragment" />
    <fragment
        android:id="@+id/filterShoosSizesFragment"
        android:name="com.project.morestore.fragments.FilterShoosSizesFragment"
        android:label="FilterShoosSizesFragment" />
    <fragment
        android:id="@+id/filterKidsSizesFragment"
        android:name="com.project.morestore.fragments.FilterKidsSizesFragment"
        android:label="FilterKidsSizesFragment" />
    <fragment
        android:id="@+id/cabinetGuestFragment"
        android:name="com.project.morestore.fragments.CabinetGuestFragment"
        android:label="CabinetGuestFragment" >
        <action
            android:id="@+id/action_cabinetGuestFragment_to_loginDialog"
            app:destination="@id/loginDialog" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.project.morestore.fragments.ProfileFragment"
        android:label="ProfileFragment" >
        <argument
            android:name="user"
            app:argType="com.project.morestore.models.User" />
    </fragment>

</navigation>